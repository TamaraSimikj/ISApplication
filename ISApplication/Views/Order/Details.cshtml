@foreach (var elem in Model.ProductInOrders)
{
    <li class="list-group-item d-flex justify-content-between align-items-start">
        <div class="ms-2 me-auto">
            <div class="fw-bold">@elem.Product.productName</div>
            <small>Expiration Date: @elem.Product.validUntil.ToShortDateString()</small>
            <br />
            <small>Movie Genre: @elem.Product.Genre</small>
            <br />
            <small>@elem.Product.productPrice * @elem.Quantity</small>
        </div>
    </li>
}